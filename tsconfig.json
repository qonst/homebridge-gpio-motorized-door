{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "sourceMap": true,
        "declaration": true,
        "outDir": "./dist",
        "strict": true,
        "noImplicitAny": true,
        "alwaysStrict": true,
        "declarationDir": "lib",
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true,
    },
    "exclude": [
        "./dist",
        "./lib"
    ]
}